
generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Post {
	id String @id @default(uuid())
	title String
	content String
	
	@@map("posts")

	createdAt DateTime @default(now())
	updatedAt DateTime @updatedAt
	
}